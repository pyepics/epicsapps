# A FAKECORP 1 model vacuum gauge, as installed in regular sections of the device

record(ai, "$(section_name):vacuum") {
  field(DTYP, "asynInt32")
  field(INP, "@asyn($(port))")
  field(LINR, "LINEAR")
  field(EGU, "Torr")
  field(EGUL, "0")
  field(EGUF, "1")
  field(PREC, "8")
  field(HIGH, "$(warning_level)")
  field(HSV, "MAJOR")
}
