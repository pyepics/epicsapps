# This database contains fields used to calculate ion chamber currents
grecord(stringin,"$(P)$(Q):AmpPV") {
        field(DESC, "IonChamber SRS570 Amp")
        field(VAL,  "13IDC:A1")
}

grecord(stringin,"$(P)$(Q):EnergyPV") {
        field(DESC, "PV for Energy Value")
        field(VAL,  "13IDA:E:E_RBV")
}


grecord(stringin,"$(P)$(Q):VoltPV") {
        field(DESC, "IonChamber Voltage PV")
        field(VAL,  "")
}

grecord(ao,"$(P)$(Q):Volts") {
        field(DESC, "IonChamber Voltage")
        field(VAL,  "")
}

grecord(ao,"$(P)$(Q):Energy") {
        field(DESC,"x-ray energy (eV)")
        field(PREC, "2")
        field(VAL, "10000")
}

grecord(stringin,"$(P)$(Q):Desc") {
        field(DESC, "Description")
        field(VAL,  "Ion Chamber 1")
}

grecord(ao,"$(P)$(Q):Length") {
        field(DESC,"IonChamber Length (cm)")
        field(PREC, "1")
        field(VAL, 10.0)
}

grecord(ao,"$(P)$(Q):AbsPercent") {
        field(DESC,"IonChamber absorbed")
        field(PREC,"2")
        field(VAL,"0")
}

grecord(mbbi,"$(P)$(Q):Gas") {
        field(DESC,"IonChamber Gas")
        field(VAL,"1")
        field(ZRVL,"0")
        field(ZRST,"Air")
        field(ONVL,"1")
        field(ONST,"N2")
        field(TWVL,"2")
        field(TWST,"He")
        field(THVL,"3")
        field(THST,"Ar")
        field(FRVL,"4")
        field(FRST,"Kr")
}

grecord(ao, "$(P)$(Q):Current") {
        field(DESC,"IonChamber Current (uA)")
        field(PREC,"5")
        field(VAL, 0.)
}

grecord(ao, "$(P)$(Q):FluxAbs") {
        field(DESC,"IonChamber Flux Absorbed (Hz)")
        field(PREC,"0")
        field(VAL, 0)
}

grecord(ao, "$(P)$(Q):FluxOut") {
        field(DESC,"IonChamber Flux Out (Hz)")
        field(PREC,"0")
        field(VAL, 0)
}

grecord(stringout, "$(P)$(Q):FluxOutS") {
        field(DESC,"IonChamber Flux Out (Hz)")
        field(VAL, "0")
}

grecord(stringout,"$(P)$(Q):TimeStamp") {
        field(DESC, "last update time")
        field(VAL,  "")
}
